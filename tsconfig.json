{
	"compilerOptions": {
	  "target": "ES2020",
	  "module": "CommonJS",  // Changé depuis NodeNext
	  "moduleResolution": "node",  // Changé depuis NodeNext
	  "outDir": "./backend/dist",
	  "rootDir": "./backend",
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "baseUrl": "./backend/src",  // Ajouté
	  "paths": {
		"@/*": ["./*"]  // Ajouté pour les alias
	  }
	},
	"include": ["backend/src/**/*"],
	"exclude": ["node_modules", "frontend"]
  }